{
  "name": "cosmos-orm",
  "version": "1.0.0",
  "description": "a simple ORM for Cosmos DB",
  "main": "./dist/index.mjs",
  "scripts": {
    "build": "tsup --dts",
    "dev": "tsup --watch",
    "lint": "biome check --apply ."
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": ["cosmos", "orm"],
  "author": "Travis Reynolds <travis@travisreynolds.dev>",
  "devDependencies": {
    "@biomejs/biome": "1.6.0",
    "@types/node": "^20.11.26",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/functions": "^4.3.0",
    "ulidx": "^2.3.0"
  }
}
